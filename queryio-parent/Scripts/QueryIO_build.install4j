String hname = "";
if(Util.isWindows())
{
   flag = true;
   flagMapping = true;
}
else if(Util.isMacOS())
{
    Process p = Runtime.getRuntime().exec("sysctl kern.sysv.shmmax");
    String shm = "";
    BufferedReader br = new BufferedReader(new InputStreamReader(p.getInputStream()));
    String line = null;
    while((line=br.readLine())!=null)
    {
        if(line.contains("kern.sysv.shmmax"))
        {
            shm = line.substring(line.indexOf(":") + 2 );
            break;
        }            
    }
    shm = shm.trim();
    s = Long.parseLong(shm);    
    if( s &gt;= 268435456)
    {            
        flag = true;
    }
    else
    {           
        flag = false;
    }
    br.close();
}   
else if(Util.isLinux())
{
    Process p = Runtime.getRuntime().exec("sysctl kernel.shmmax");
    String shm = "";
    BufferedReader br = new BufferedReader(new InputStreamReader(p.getInputStream()));
    String line = null;
    while((line=br.readLine())!=null)
    {
        if(line.contains("kernel.shmmax"))
        {
            shm = line.substring(line.indexOf("=") + 2 );
            break;
        }            
    }
    shm = shm.trim();
    s = Long.parseLong(shm);    
    if( s &gt;= 268435456)
    {           
        flag = true;
    }
    else
    {           
        flag = false;
    }
    br.close();
} 



if(clusterSetup.equals("yes") &amp;&amp; !Util.isWindows())
