            <validation>Integer type = 1; // linux
context.setVariable("REMOTE_MACHINE", 1);
String user = (String)context.getVariable("SSH_USER");
String password = (String)context.getVariable("SSH_PASSWORD");
String host = (String)context.getVariable("SSH_HOSTNAME");
String port = (String)context.getVariable("SSH_PORT");
boolean remoteWindowsOS = false;
if(type.equals(0)||type.equals(1)){
   
    remoteWindowsOS = false;
    if(type.equals(0)){
        context.setVariable("INSTALLATION_DIR","/Users/"+user);
    }
    else{
        context.setVariable("INSTALLATION_DIR","/home/"+user);
    }
}
else{
    type = 2;
    remoteWindowsOS = true;
    context.setVariable("INSTALLATION_DIR","C:\\");
}
context.setVariable("REMOTE_WINDOWS_OS",remoteWindowsOS);


boolean flag = com.queryio.remote.validation.RemoteValidationAction.validateConnection(host, port, user, password,type);
if(flag)
{
    if(Util.isWindows()){
        context.setVariable("TEMP_DIR","C:\\temp");
    }
    else{
        String user_dir = System.getProperty("user.home"); 
        context.setVariable("TEMP_DIR", user_dir+"/temp");
    }
   String jre_home = com.queryio.remote.validation.JREValidation.findJRE(host, port, user, password,type);
   if(jre_home != null)
   {
       context.setVariable("JRE_HOME",jre_home);
   }
   else{
       context.setVariable("JRE_HOME","");
   }
   if(remoteWindowsOS)
   {
        Util.showMessage("Windows machine will only support QueryIO UI server. Cluster setup requires Linux / Mac OS machines.");
   }
   context.setVariable("CLUSTER_USERHOME" , context.getVariable("INSTALLATION_DIR"));
   return true;
}
else{
    Util.showErrorMessage("Connection to Remote Machine Failed.");
    return false;
}
</validation>
