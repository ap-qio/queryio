String ip_private_key = (String)context.getVariable("IP_PRIVATE_KEY");;//(String)context.getVariable("IP_PRIVATE_KEY");
String ip_port =(String)context.getVariable("IP_PORT");

// 1 - password
// 2 - private key
int auth_type = (Integer) context.getVariable("IP_AUTHENTICATION_TYPE");
String msg = "";
boolean flag = true;

System.out.println("auth_type : " + auth_type);

if(auth_type == 0)
{
    context.setVariable("IP_PRIVATE_KEY","null");
    ip_private_key = null;
}
else
{
    context.setVariable("IP_PASSWORD", "null");
    ip_password = null;
}


if(context.getVariable("CLUSTER_INSTALL_LOC").equals("remote"))
{
    if(  (ip.equals("") || ip_username.equals("") || ip_port.equals("")) )
    {
        Util.showErrorMessage("Please provide SSH details for host machine.");
        return false;
    }
    boolean validate = false;
    validate = com.queryio.uninstall.kill.RemoteFunction.validateConnectionUninstall(ip, ip_port, ip_username, ip_password, ip_private_key);
    if(!validate)
    {
         Util.showErrorMessage("Connection to Remote Machine Failed.");
         return false;
    }
}



if(context.getVariable("CLUSTER_INSTALL_LOC").equals("local"))
{
    context.setVariable("IP" , "127.0.0.1");//(String) context.getVariable("SSH_HOSTNAME"));
    context.setVariable("SETUP_LOCATION" , true);
    context.setVariable("DB_HOSTNAME" , "127.0.0.1");
}
else
{
    context.setVariable("SETUP_LOCATION" , false);
    context.setVariable("DB_HOSTNAME" , (String) context.getVariable("SSH_HOSTNAME") );    
}

if(!msg.equals(""))
{
     Util.showErrorMessage(msg);
}

System.out.println("\n");
return flag;
