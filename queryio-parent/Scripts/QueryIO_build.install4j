        Process p = Runtime.getRuntime().exec("hostname");            
        BufferedReader br = new BufferedReader(new InputStreamReader(p.getInputStream()));
        String line = null;
        while((line=br.readLine())!=null)
        {
           hname = line;           
        }
        br.close();
        p.destroy();
       //hostName = ownIP.getHostName();
        Process p1 = Runtime.getRuntime().exec("cat /etc/hosts");            
        br  = new BufferedReader(new InputStreamReader(p1.getInputStream()));
        line = null;
        while((line=br.readLine())!=null)
        {
            if ((!line.startsWith("#")) &amp;&amp; line.contains(hostName) &amp;&amp; line.contains(hostIP))               
            {
                flagMapping = true;
            }            
        }
        br.close();
        p1.destroy();
   } 
   else 
   {
       String ip = (String) context.getVariable("IP");
       String username = (String) context.getVariable("IP_USERNAME");
       String password = (String) context.getVariable("IP_PASSWORD");
       String ip_private_key =(String)context.getVariable("IP_PRIVATE_KEY");
       String ip_port =(String)context.getVariable("IP_PORT");
       if(ip_private_key.equals("null"))
           ip_private_key = null;
        if(password.equals("null"))
           password = null;
       flagMapping = com.queryio.remote.validation.RemoteValidationAction.IPMapping(ip, ip_port, username, password, ip_private_key);
   }
} else {
   flagMapping = true;
}


context.setVariable("SHM_VALUE" , s);
context.setVariable("HOSTIP", hostIP); //FIXME
context.setVariable("HOSTNAME", hname); // FIXME

//flag = true, means no issue found in shmax value
if(flag) {
    context.setVariable("SHMAX_ISSUE" , "false");
} else {
   context.setVariable("SHMAX_ISSUE" , "true"); 
}

//flagMapping = true, means IP-Hostname mapping found in /etc/hosts file
if(flagMapping) {
    context.setVariable("HOST_ISSUE" , "false");    
} else {
    context.setVariable("HOST_ISSUE" , "true");
}



return (!flag || !flagMapping);</condition>
